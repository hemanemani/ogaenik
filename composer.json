{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "version": "11.31",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aiz-packages/color-code-converter": "dev-main",
        "aiz-packages/combination-generate": "dev-main",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "authorizenet/authorizenet": "^2.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "enshrined/svg-sanitize": "^0.15.4",
        "genealabs/laravel-sign-in-with-apple": "*",
        "guzzlehttp/guzzle": "^7.5",
        "instamojo/instamojo-php": "^0.4.0",
        "intervention/image": "^2.5",
        "iyzico/iyzipay-php": "^2.0",
        "kingflamez/laravelrave": "dev-master",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^11.31",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.2",
        "lcobucci/jwt": "^4.0",
        "league/flysystem-aws-s3-v3": "^3.12",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.1",
        "myfatoorah/laravel-package": "^2.2",
        "paypal/paypal-server-sdk": "^0.6.1",
        "predis/predis": "^2.1",
        "psr/log": "^2.0",
        "razorpay/razorpay": "2.*",
        "rmccue/requests": "^2.0",
        "sebacarrasco93/laravel-payku": "^1.0",
        "spatie/db-dumper": "^3.3",
        "spatie/laravel-permission": "^6.10",
        "stripe/stripe-php": "^10.5",
        "twilio/sdk": "^6.44",
        "unicodeveloper/laravel-paystack": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "dev-master",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
